   <pattern id="schematron-constraint-p5odds-content-only_1_child_of_content-115">
      <rule context="tei:content[ *[2] ]">
         <let name="tot_kids" value="count( * )"/>
         <let name="tei_kids" value="count( tei:* )"/>
         <let name="rng_kids" value="count( rng:* | rna:* )"/>
         <let name="msg_part01"
              value="'In the near future the &lt;content&gt; element will be limited to 1 and only 1 child element. '"/>
         <let name="msg_part02" value="'This &lt;content&gt; element is in '"/>
         <let name="msg_part03"
              value="if ( local-name(..) eq 'elementSpec' ) then 'an ' else 'a '"/>
         <let name="msg_part04" value="concat( local-name(..), ' with ' )"/>
         <let name="msg_part05"
              value="if ( parent::tei:moduleRef/@key ) then 'a @key'    else ''"/>
         <let name="msg_part06"
              value="if ( parent::tei:moduleRef/@url ) then 'a @url'    else ''"/>
         <let name="msg_part07"
              value="if ( parent::tei:*/@ident )       then 'an @ident' else ''"/>
         <let name="msg_part08" value="' of &#34;'"/>
         <let name="msg_part09" value="../@ident | ../@key | ../@url"/>
         <let name="msg_part10" value="concat('&#34; and has ', $tot_kids, ' children,')"/>
         <let name="msgs_1to10"
              value="concat( $msg_part01, $msg_part02, $msg_part03, $msg_part04, $msg_part05, $msg_part06, $msg_part07, $msg_part08, $msg_part09, $msg_part10 )"/>
         <report test="$tei_kids eq $tot_kids" role="warning">
            <value-of select="$msgs_1to10"/> which could be wrapped in a &lt;sequence&gt; element.
      </report>
         <report test="$rng_kids eq $tot_kids" role="warning">
            <value-of select="$msgs_1to10"/> which could be wrapped in an &lt;rng:div&gt; element.
      </report>
         <assert test="$tei_kids eq $tot_kids  or  $rng_kids eq $tot_kids"
                 role="warning">
            <value-of select="$msgs_1to10"/> but those children are neither all TEI elements nor
        all RELAX NG elements, and thus this &lt;content&gt; is invalid and can not be easily rectified.
      </assert>
      </rule>
   </pattern>
   <pattern id="schematron-constraint-p5odds-sequence-sequencechilden-116">
      <rule context="tei:sequence">
         <assert test="count(*)&gt;1">The sequence element must have at least two child elements</assert>
